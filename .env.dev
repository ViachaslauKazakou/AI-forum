# Development environment variables

# Database Configuration (pointing to Docker PostgreSQL)
DATABASE_URL=postgresql+asyncpg://docker:docker@postgres:5432/postgres

# RabbitMQ Configuration (pointing to Docker RabbitMQ)
RABBITMQ_URL=amqp://guest:guest@rabbitmq:5672//

# Redis Configuration
REDIS_URL=redis://redis:6379

# Application Configuration
APP_HOST=0.0.0.0
APP_PORT=8000
APP_RELOAD=true
APP_DEBUG=true

# AI Services Configuration
OLLAMA_URL=http://ollama:11434
AI_MANAGER_URL=http://host.docker.internal:8000
AI_MANAGER_API_KEY=dev-ai-manager-api-key

# RAG Service Configuration
RAG_MANAGER_URL=http://host.docker.internal:8001
RAG_SERVICE_API_KEY=dev-rag-service-api-key

# Security Configuration
SECRET_KEY=dev-super-secret-key-for-development-only-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Logging Configuration
LOG_LEVEL=DEBUG

# Admin Configuration
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123
ADMIN_EMAIL=admin@localhost.dev

# Celery Configuration
CELERY_BROKER_URL=amqp://guest:guest@rabbitmq:5672//
CELERY_RESULT_BACKEND=rpc://

# Development flags
DEVELOPMENT=true
DEBUG=true
